@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">DarkFactor : Bugreports </h1>

    <center>
    <table width=500 border=1>
    @foreach (var bugreport in Model.bugReportList)
    {
        <TR>
            <TD width=50 align=center><a href="/Bugreport?bugReportId=@bugreport.clientBugId">@bugreport.clientBugId</a></TD>
            <TD>&nbsp; @bugreport.title</TD>
            <TD width=100 align=center>@bugreport.created.Day @bugreport.created.Month @bugreport.created.Year </TD>
        </TR>
    }
    </table>
    </center>
</div>
